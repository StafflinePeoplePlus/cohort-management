{
	"name": "@peopleplus/cohort-management",
	"version": "0.4.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "eslint .",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"codegen": "graphql-codegen --config codegen.ts && node scripts/patchGeneratedTypes.js && node scripts/createSchemaExport.js",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"release": "npm run build && changeset publish"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"svelte": "./dist/client.js"
		},
		"./server": {
			"types": "./dist/server.d.ts",
			"import": "./dist/server.js"
		}
	},
	"typesVersions": {
		"*": {
			"index.d.ts": [
				"dist/index.d.ts"
			],
			"client": [
				"dist/client.d.ts"
			],
			"server": [
				"dist/server.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"schema.graphql",
		"defaults.graphql"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/StafflinePeoplePlus/cohort-management.git"
	},
	"author": "James Birtles <james.birtles@peopleplus.co.uk>",
	"bugs": {
		"url": "https://github.com/StafflinePeoplePlus/cohort-management/issues"
	},
	"homepage": "https://github.com/StafflinePeoplePlus/cohort-management#readme",
	"peerDependencies": {
		"svelte": "^4.0.0",
		"@sveltejs/kit": "^1.0.0 || ^2.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@faker-js/faker": "^8.3.1",
		"@graphql-codegen/add": "^5.0.0",
		"@graphql-codegen/cli": "5.0.2",
		"@graphql-codegen/typescript": "4.0.6",
		"@graphql-codegen/typescript-resolvers": "4.0.6",
		"@graphql-tools/executor-http": "^1.0.5",
		"@storybook/addon-essentials": "^7.6.4",
		"@storybook/addon-interactions": "^7.6.4",
		"@storybook/addon-links": "^7.6.4",
		"@storybook/blocks": "^7.6.3",
		"@storybook/svelte": "^7.6.4",
		"@storybook/sveltekit": "^7.6.4",
		"@storybook/testing-library": "^0.2.2",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.2.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-svelte": "^2.35.1",
		"graphql-yoga": "^5.0.2",
		"postcss": "^8.4.32",
		"prettier": "~3.2.5",
		"prettier-plugin-svelte": "~3.2.1",
		"publint": "^0.2.6",
		"storybook": "^7.6.4",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.0",
		"vitest": "^1.0.0"
	},
	"dependencies": {
		"graphql": "^16.8.1",
		"lucide-svelte": "^0.354.0",
		"@peopleplus/components": "^0.1.0",
		"tailwind-merge": "^2.1.0",
		"type-fest": "^4.8.3"
	},
	"overrides": {
		"@storybook/svelte-vite": {
			"vite": "$vite"
		},
		"@storybook/sveltekit": {
			"vite": "$vite"
		}
	}
}

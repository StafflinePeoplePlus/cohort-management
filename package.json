{
	"name": "@peopleplus/cohort-management",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"codegen": "graphql-codegen --config codegen.ts && node scripts/patchGeneratedTypes.js && node scripts/createSchemaExport.js"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"svelte": "./dist/client.js"
		},
		"./server": {
			"types": "./dist/server.d.ts",
			"import": "./dist/server.js"
		}
	},
	"typesVersions": {
		"*": {
			"index.d.ts": [
				"dist/index.d.ts"
			],
			"client": [
				"dist/client.d.ts"
			],
			"server": [
				"dist/server.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"schema.graphql",
		"defaults.graphql"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/StafflinePeoplePlus/cohort-management.git"
	},
	"author": "James Birtles <james.birtles@peopleplus.co.uk>",
	"bugs": {
		"url": "https://github.com/StafflinePeoplePlus/cohort-management/issues"
	},
	"homepage": "https://github.com/StafflinePeoplePlus/cohort-management#readme",
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@graphql-codegen/add": "^5.0.0",
		"@graphql-codegen/cli": "5.0.0",
		"@graphql-codegen/typescript": "4.0.1",
		"@graphql-codegen/typescript-resolvers": "4.0.1",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.6",
		"@sveltejs/package": "^2.2.2",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.1",
		"publint": "^0.2.5",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^4.4.2",
		"vitest": "^0.34.6"
	},
	"dependencies": {
		"graphql": "^16.8.1",
		"lucide-svelte": "^0.292.0",
		"pp-svelte-components": "0.0.0-snapshot-20231122151226",
		"tailwind-merge": "^2.0.0",
		"type-fest": "^4.8.2"
	}
}
